@{
    ViewBag.Title = "Index";
    List<TVHS.ViewModels.ViewModelProgram> progamList = ViewBag.originalProgram;
    List<TVHS.ViewModels.ViewModelProgram> progamList2 = ViewBag.realProgram;

    List<TVHS.ViewModels.ViewModelProgram> progamListnext = ViewBag.originalProgramnext;
    List<TVHS.ViewModels.ViewModelProgram> progamList2next = ViewBag.realProgramnext;

    List<TVHS.ViewModels.ViewModelProgram> predictResult = ViewBag.predictResult;
    List<TVHS.ViewModels.ViewModelProgram> realResult = ViewBag.realresult ;

    List<TVHS.ViewModels.ViewModelProgram> compare = ViewBag.compare;
}
<br />
<p>Date: 30/08/2015</p>
<table class="table">
    <tr>
        <th>
            No
        </th>
        <th>
            Program Code
        </th>
        <th>
            Name
        </th>             
        <th>
            Duration
        </th>
        <th></th>
    </tr>

    @foreach (var item in progamList)
    {
        <tr>
            <td>
                @(progamList.IndexOf(item)+1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgramCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>        
            <td>
                @Html.DisplayFor(modelItem => item.Duration)
            </td>
           
        </tr>
    }
</table>
