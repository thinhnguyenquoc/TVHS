<p>Result of prediction on 31/08/2015</p>
<table>
    <tr>
        <th>Code</th>
        <th>Name</th>
        <th>No show times</th>
        <th>Predicted quantity</th>
    </tr>
    @for (var i = 0; i < predictResult.Count(); i++)
    {
    <tr>
        <td width="13%">
            @predictResult[i].ProgramCode
        </td>
        <td width="60%">
            @predictResult[i].Name
        </td>
        <td align="center" width="13%">
            @predictResult[i].quantityList.FirstOrDefault().NoTimes
        </td>
        <td align="center">
            @predictResult[i].quantityList.FirstOrDefault().quantity
        </td>
    </tr>
    }
</table>
